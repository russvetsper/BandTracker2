<h1>Venues</h1>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var venue in Model)
    {
      <li><a href='/venues/@venue.GetId()'>@venue.GetName()</a>
	| <a href="/venue/edit/@venue.GetId()">Edit</a>
	| <a href="venue/delete/@venue.GetId()">Delete</a>
      </li>
    }
  </ul>
  <form action="/venues/delete" method="post">
    <button type="submit">Clear venues</button>
  </form>
}
<h3><a href="/venues/new">Add venue</a></h3>
<h3><a href="/">Return Home</a></h3>
